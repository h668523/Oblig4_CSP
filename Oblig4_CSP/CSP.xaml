<Window x:Class="Oblig4_CSP.CSP"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Oblig4_CSP"
        mc:Ignorable="d"
        Title="CSP" Height="450" Width="800">
    <Grid>
        <ListView Name="CSPList" Margin="10,10,10,10" MouseDoubleClick="CSPList_MouseDoubleClick">
            <ListView.View>
                <GridView>
                    <GridViewColumn Header="Room" Width="100" DisplayMemberBinding="{Binding room.RoomNr}"/>
                    <GridViewColumn Header="Status" Width="100" DisplayMemberBinding="{Binding status}"/>
                    <GridViewColumn Header="Task Descritpion" Width="400" DisplayMemberBinding="{Binding description}"/>
                </GridView>
            </ListView.View>
        </ListView>
    </Grid>
</Window>
